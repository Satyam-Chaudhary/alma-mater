generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider =  "mongodb"
  url      =  env("DATABASE_URL")
}

model CollegesName {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  established Int
  name        String


}

model CollegeDetails {
  id          String       @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  numberAlumni Int
  
 
}
