generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model CollegeDetails {
  id                 String @id @default(auto()) @map("_id") @db.ObjectId
  collegeDescription String
  collegeId          String @db.ObjectId
  collegeName        String
  numberOfAlumni     Int
}

model CollegesName {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  established Int
  name        String
}
